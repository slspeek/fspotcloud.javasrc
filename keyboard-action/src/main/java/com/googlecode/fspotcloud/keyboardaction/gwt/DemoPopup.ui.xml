<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="com.googlecode.fspotcloud.keyboardaction.HelpResources" field="res"></ui:with>

    <g:FocusPanel ui:field="focusPanel">
        <g:HTMLPanel>
            <div ui:field="titleDiv" class="{res.style.helpTitle}"></div>
            <div ui:field="helpBodyDiv" class="{res.style.helpBody}"></div>
            <g:Anchor ui:field="stopAnchor" styleName="{res.style.helpClose">Stop demo</g:Anchor>
        </g:HTMLPanel>
    </g:FocusPanel>
</ui:UiBinder>