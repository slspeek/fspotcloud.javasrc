<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='fspotcloud'>
    <!-- Inherit the core Web Toolkit stuff. -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name='com.googlecode.fspotcloud.rpc'/>
    <inherits name="com.google.gwt.user.Debug"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
    <!--<inherits name='com.google.gwt.user.theme.dark.Dark'/>-->
    <!-- GWT dispatch -->
    <inherits name='net.customware.gwt.dispatch.Dispatch'/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.junit.JUnit"/>
    <!-- Other module inherits -->
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.reveregroup.gwt.imagepreloader.ImagePreloader"/>
    <!-- Specify the app entry point class. -->
    <entry-point class="com.googlecode.fspotcloud.client.main.Main"></entry-point>

    <set-property name="gwt.logging.popupHandler" value="DISABLED"/>
</module>
