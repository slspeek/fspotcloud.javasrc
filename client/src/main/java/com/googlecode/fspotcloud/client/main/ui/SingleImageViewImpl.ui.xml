<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:my="urn:import:com.googlecode.fspotcloud.client.main.ui">
    <ui:style>
        table {
        margin: auto;
        }
    </ui:style>
    <ui:with type="com.googlecode.fspotcloud.client.main.ui.Resources" field="res"></ui:with>
    <g:LayoutPanel styleName="{res.style.slideshow}" ui:field="layout">
        <g:layer>
            <my:ImageRasterViewImpl></my:ImageRasterViewImpl>
        </g:layer>
        <g:layer left="25%" right="25%" bottom="0cm" height="0px">
            <my:ButtonPanelViewImpl styleName="{res.style.slideshowControls}"></my:ButtonPanelViewImpl>
        </g:layer>
    </g:LayoutPanel>
</ui:UiBinder> 
