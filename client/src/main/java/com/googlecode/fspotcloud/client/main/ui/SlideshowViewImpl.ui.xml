<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:my="urn:import:com.googlecode.fspotcloud.client.main.ui"
             xmlns:ka="urn:import:com.googlecode.fspotcloud.keyboardaction.gwt">
    <ui:style>
        table {
        margin: auto;
        }
    </ui:style>
    <ui:with type="com.googlecode.fspotcloud.client.main.ui.Resources" field="res"></ui:with>
    <g:LayoutPanel styleName="{res.style.slideshow}" ui:field="layout">
        <g:layer>
            <my:DoubleImageViewImpl></my:DoubleImageViewImpl>
        </g:layer>
        <g:layer left="22%" right="22%" bottom="0cm" height="0px">
            <ka:ActionToolbar></ka:ActionToolbar>
        </g:layer>
    </g:LayoutPanel>
</ui:UiBinder> 
